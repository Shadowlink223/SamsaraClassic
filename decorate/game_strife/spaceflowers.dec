    DamageFactor "ZorchSpectral", 1
    DamageFactor "SpectralNoQuad", 1

    DropItem ""
    Args 1

    States
    {
      See:
        TNT1 A 0 A_JumpIfInventory("DukeBossSee",1,"See2")
        TNT1 A 0 A_GiveInventory("DukeBossSee",1)
