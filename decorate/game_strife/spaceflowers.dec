      Death.DukeDamage:
      Death.MightyBoot:
      Death.DukeExplosive:
      Death.DukeSpectral:
      Death.DukePistol:
      Death.DukeIce:
        TNT1 A 0 A_GiveToTarget("DukeBossKillTaunt",1)
        
